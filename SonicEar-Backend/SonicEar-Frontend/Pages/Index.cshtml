@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div id="app">
    <div class="row">
        <h3>
            Dashboard
        </h3>
    </div>
    <div class="row">
        <div>
            <h4>Enheder</h4>
        </div>
        <div>
            <div style="display: inline-block; margin-inline-end: 25px;" v-for="item in items">
                <div style="width: 75px!important; height: 100px!important; background-color: #e7e7e7;">
                </div>
                <p style="text-align:center">{{item.location}}</p>
            </div>
        </div>
    </div>
    <div class="row">
        <div>
            <h4>Seneste målinger</h4>
        </div>
    </div>
</div>

<script src="~/js/devices.js"></script>